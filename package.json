    "@types/string-hash": "^1.1.1",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-react-require": "^3.1.3",
    "classnames": "^2.2.6",
    "next": "^10.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "string-hash": "^1.1.3",
